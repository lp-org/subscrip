        "seed": "ts-node-dev src/drizzle-seed.ts"
    },
    "dependencies": {
        "awilix": "^8.0.1",
        "cross-spawn": "^7.0.3",
        "ts-node-dev": "^2.0.0",
        "turbowatch": "^2.29.4",
        "winston": "^3.10.0",
        "db":"*",
        "server":"*"
    },
    "devDependencies": {
        "@types/cross-spawn": "^6.0.2"
    }
}
