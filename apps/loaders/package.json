{
    "name": "loaders",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "node dist/index.js",
        "build": "npm run clean && tsc -p tsconfig.json",
        "watch":"tsc --watch",
        "dev": "ts-node-dev --respawn -- src/index.ts",
